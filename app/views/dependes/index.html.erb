<p id="notice"><%= notice %></p>

<h1>Dependes</h1>

<table>
  <thead>
    <tr>
      <th>Cpf func</th>
      <th>Nome</th>
      <th>Relacao</th>
      <th>Datanasc</th>
      <th>Sexo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dependes.each do |depende| %>
      <tr>
        <td><%= depende.cpf_func %></td>
        <td><%= depende.nome %></td>
        <td><%= depende.relacao %></td>
        <td><%= depende.datanasc %></td>
        <td><%= depende.sexo %></td>
        <td><%= link_to 'Show', depende %></td>
        <td><%= link_to 'Edit', edit_depende_path(depende) %></td>
        <td><%= link_to 'Destroy', depende, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Depende', new_depende_path %>
